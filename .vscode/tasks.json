{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Run TensorBoard",
            "type": "shell",
            "command": "tensorboard --logdir=logs/tensorboard",
        },
        {
            "label": "Clear TensorBoard Logs",
            "type": "shell",
            "command": "rm -rf logs/tensorboard/train/*",
        },
        {
            "label":"Run Neo4j",
            "type": "shell",
            "command": "sudo neo4j console"
        },
        {
            "label": "Run Apollo Server",
            "type": "shell",
            "command": "cd apollo; npm run start",
        },
        {
            "label":"Run Flask GUI",
            "type": "shell",
            "command": "cd example/gui;python3 app.py"
        }
    ]
}